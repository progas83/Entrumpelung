<!-- ------------- 1st block SERVICES --------------- -->
@{
    string cityName = "Minden";
    string fullTel1Number = "0571. 829 45 878";
    string tel1 = "";
    string cityCode = "";
    if (Request.Cookies["City"] != null)
    {
        cityName = Server.UrlDecode(Request.Cookies["City"].Value);
    }


    if (Request.Cookies["Tel1"] != null && Request.Cookies["CityCode"] != null)
    {
        tel1 = Request.Cookies["Tel1"].Value;
        cityCode = Request.Cookies["CityCode"].Value;

    }
    fullTel1Number = string.Format("{0}. {1}", cityCode, tel1);
    string sityNameWrapper = "<span class='currentCity'>" + @cityName + "</span>";
    string inCityName = " in " + @sityNameWrapper;
}

<div class="box content">

    <h1>Entrümpelung @Html.Raw(@inCityName) </h1>
    <p>Fachbetrieb für Entrümpelung - Rudi Rümpel</p>

    <ol>
        <li>Schnell</li>
        <li>Sauber
        <li>Seriös</li>
    </ol>
    <h3>Es steht eine Entrümpelung an?</h3>
    <p>
        Rudi Rümpel, Entrümpelung @Html.Raw(@inCityName). Wir helfen Ihnen! Wir räumen auf und hinterlassen nur Sauberkeit!
    </p>
    <h2>Wir beschäftigen uns mit:</h2>

    <div class="serviceWrapper">
        <div class="serviceItems">
            <img src="~/Content/IMG/service1.png" />
            <a href="/Entrumpelung">Entrümpelung</a>
        </div>
        <div class="serviceItems">
            <img src="~/Content/IMG/service2.png" />
            <a href="/Haushaltsauflosung">Haushaltsauflösung</a>
        </div>
        <div class="serviceItems">
            <img src="~/Content/IMG/service3.png" />
            <a href="/Wohnungsauflosung">Wohnungsauflösung</a>
        </div>
        <div class="serviceItems">
            <img src="~/Content/IMG/service4.png" />
            <a href="/Wohnungsraumung">Wohnungsräumung</a>
        </div>
        <div class="serviceItems">
            <img src="~/Content/IMG/service5.png" />
            <a href="/Tatortreiniger">Tatortreiniger</a>
        </div>
    </div>

</div>
<!-- ------------- /1st block SERVICES --------------- -->
<!-- ------------- 2nd block CALLBACK --------------- -->
<div class="full-area callBack">
    <div class="box">
        <div class="CBPhonesWrapper">
            <p class="tittle">
                <a class="cityChange" href="#city">@Html.Raw(@sityNameWrapper)</a>
            </p>
            <div class="headPnonesBlock">
                <div class="headPhonesImg1"></div><div class="headPhonesNumbers"><a class="cityTel" href="tel:@cityCode@tel1">@fullTel1Number</a></div>
                <div class="headPhonesImg2"></div><div class="headPhonesNumbers"><a class="cityFax" href="tel:057182945879">0571. 829 45  879</a></div>
            </div>
        </div>
        <div class="CBArrow"></div>
        <div class="CBPFormWrapper">

            <div class="CBInputWrapper">
                <p class="tittle">Noch Fragen?</p>
                <p class="tittle">Wir rufen Sie zurück!</p>
                <div id="divEmp">
                    @using (Ajax.BeginForm("CallBackOrder", new AjaxOptions { HttpMethod = "POST", UpdateTargetId="divEmp", InsertionMode = InsertionMode.Replace}))
                    {
                        <input name="name" placeholder="Name" />
                        <input name="telephone" placeholder="Telefon*" />
                        <input class="buttonSubmit" type="submit" value="Rückruf">
                    }
                </div>
            </div>
        </div>
    </div>
</div>
<!-- ------------- /2nd block CALLBACK --------------- -->
