<script type="text/javascript" language="javascript">
  //function SmartScroll(event) {
  //      console.log("In function");
  //      event.preventDefault();
  //}

  $(document).ready(function () {
      var i,k;
      var allAElements = document.getElementsByTagName('a');
      for (i = 0; i <allAElements.length; i++)
      {
          if(allAElements[i].getAttribute("href").charAt(0)=='#')
          {
              var aRefName = allAElements[i].getAttribute("href").substring(1);
              for(k=0;k<allAElements.length;k++)
              {
                  if(allAElements[k].getAttribute("name")==aRefName)
                  {
                      var elemTop = $(allAElements[k]).offset().top;
                      console.info(elemTop);

                      allAElements[i].addEventListener("click", setHandler(elemTop), false);
                  }
              }
          }
      }
  })
  function setHandler(elemTop) {
      return function SmartScroll(event) {
          event.preventDefault();
          console.info(elemTop);

          $('body,html').animate({ scrollTop: elemTop - 60 }, 1500);
      }
  }
</script>

